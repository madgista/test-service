<?xml version="1.0" encoding="UTF-8"?>
<ruleset comparisonMethod="maven"
         xmlns="https://www.mojohaus.org/VERSIONS/RULE/2.1.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://www.mojohaus.org/VERSIONS/RULE/2.1.0
         https://www.mojohaus.org/versions/versions-model/xsd/rule-2.1.0.xsd">
    <ignoreVersions>
        <ignoreVersion type="regex">(?i).*[-_\.](alpha|b|beta|rc|m|ea|nightly)[-_\.]?[0-9]*</ignoreVersion>
        <ignoreVersion type="regex">.*-SNAPSHOT</ignoreVersion>
    </ignoreVersions>
</ruleset>
